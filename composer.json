{
  "name": "pxgamer/devrant-php",
  "type": "library",
  "description": "A simple PHP wrapper for utilising the devRant API.",
  "keywords": ["php", "devRant", "api", "wrapper"],
  "homepage": "https://github.com/PXgamer/devrant-php",
  "license": "MIT",
  "authors": [
    {
      "name": "PXgamer",
      "email": "owzie123@gmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/PXgamer/devrant-php/issues"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/PXgamer/devrant-php"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "ext-curl": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.0",
    "phpunit/phpunit": ">=4.5.0"
  },
  "autoload": {
    "psr-4": {
      "pxgamer\\devRant\\": "src/",
      "": "vendor/"
    }
  }
}
